# Exemplo de variáveis de ambiente para desenvolvimento local
DB_HOST=localhost
DB_PORT=5432
# DEFAULT: nome do banco usado pelo container e pela app
# Mantenha como 'sosrota_db' para consistência entre todos os desenvolvedores
DB_NAME=sosrota_db
DB_USER=sosrota
DB_PASS=sosrota_pass

# Variáveis utilizadas pelo Spring (opcionais - aplicam defaults se não setadas)
DB_URL=jdbc:postgresql://${DB_HOST}:${DB_PORT}/${DB_NAME}
PORT=8081
JPA_DDL_AUTO=update
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Frontend (Vite)
VITE_API_BASE=http://backend:8081/api

# Use este arquivo como base e não comite credenciais reais. Crie um .env local para uso pessoal.